{
  "expo": {
    "name": "Teleport",
    "slug": "Teleport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.x1arty1.Teleport"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "extra": {
      "eas": {
        "projectId": "12eaaca0-f030-4a27-8a46-4ff20030f009"
      }
    }
  }
}
